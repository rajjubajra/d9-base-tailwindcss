
{{kint(content)}}

<div class="min-h-screen flex flex-col justify-center  items-center">

  <div class="w-full xl:w-1/3 lg:w-1/2 md:w-3/5 lg:py-5">
    <div class="md:grid md:grid-flow-col">
      <div class="flex justify-center md:border-r">
        <div class="sm:mb-4">
          {{ fields.field_logo_square.content }}
        </div>
      </div>

      <div class="lg:col-start-2 
                      md:pl-8 md:mt-8 
                      flex justify-center flex-col">

        
        <h1 class="text-4xl md:text-5xl tracking-tighter text-center md:text-left">{{ fields.title.content}}</h1>

      
        <div class="h-300 mt-10 flex align-middle justify-center">
          <div>
            {{ fields.body.content }}
          </div>
        </div>
      </div>
    </div>
  </div>


  <div class="w-5/6 lg:w-1/3 md:w-2/5 mt-8">
      <div class="grid md:grid-cols-2 md:grid-rows-2  gap-2">
            <div class="text-center border p-2 font-thin">
              {{ fields.field_links.content }} 
            </div>
      </div>
  </div>

</div>
